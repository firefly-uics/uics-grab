	<!-- vrv监控指标统计信息 service -->
    <dubbo:reference id="vrvTargetService" interface="com.uics.grab.service.VrvTargetService"/>
    <!-- vrv监控指标统计信息 service -->


	<!-- vrv配置信息 service -->
    <dubbo:reference id="vrvConfigService" interface="com.uics.grab.service.VrvConfigService"/>
    <!-- vrv配置信息 service -->


	<!-- idcs代办 service -->
