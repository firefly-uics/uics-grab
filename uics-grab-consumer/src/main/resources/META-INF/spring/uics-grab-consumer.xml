<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="uics-grab-consumer-consumer" owner="" organization="ruijie"/>

    <dubbo:registry address="zookeeper://101.201.196.29:2181"/>

    <!--uncomment this if you want to test dubbo's monitor-->
    <!--<dubbo:monitor protocol="registry"/>-->

    <dubbo:reference id="accountService" interface="com.uics.grab.service.AccountService"/>

	<!-- idcs通知 service -->
    <dubbo:reference id="idcsNotificationService" interface="com.uics.grab.service.IdcsNotificationService"/>
